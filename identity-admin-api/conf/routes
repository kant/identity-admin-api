GET        /healthcheck        @controllers.HealthCheckController.healthCheck

GET        /badRequest         @controllers.LoggingTestController.badRequest
GET        /internalError      @controllers.LoggingTestController.internalError
GET        /unauthorized       @controllers.LoggingTestController.unauthorized

GET        /v1/user/search     @controllers.UsersController.search(query: String, limit: Option[Int], offset: Option[Int])
GET        /v1/user/:id        @controllers.UsersController.findById(id: String)
POST       /v1/user/:id        @controllers.UsersController.update(id: String)
